<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    
     <table>
        <thead>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Mobile No.</th>
            <th>Email</th>
            <th>Street</th>
            <th>City</th>
            <th>State</th>
            <th>Country</th>
            <th>Login Id</th>
            <th>Password</th>
            <th>Created</th>
            <th>Updated</th>
        </thead>
        <% user.forEach(data=> { %>
        <tr>
         <td> <%=data.firstName  %></td>
         <td><%=data.lastName%></td>
         <td> <%=data.mobileNumber%></td>
         <td> <%=data.email%></td>
         <td><%=data.street%></td>
         <td><%=data.city%></td>
         <td><%=data.state%></td>
         <td><%=data.country%></td>
         <td><%=data.loginId%></td>
         <td><%=data.password%></td>
         <td><%=data.createdAt%></td>
         <td> <%= data.updatedAt %></td>
        </tr>
        <% }) %>
     </table>
</body>
</html>